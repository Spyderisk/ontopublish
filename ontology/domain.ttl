@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdfg: <http://www.w3.org/2004/03/trix/rdfg-1/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix score: <http://ontology.spyderisk.org/ns/core#> .
@prefix sdom: <http://ontology.spyderisk.org/ns/domain#> .

sdom:Role a rdfs:Class ;
    rdfs:label "non-pattern role property"@en .
    
sdom:has_role a rdf:Property ;
    rdfs:label "has non-pattern role property"@en ;
    rdfs:domain score:Control, score:AssetTrustworthiness, score:AssetBehaviour ;
    rdfs:range  sdom:Role .

sdom:asset_relation a rdf:Property, skos:Concept ;
    rdfs:label "relation between two assets"@en ;
    rdfs:domain score:Asset ;
    rdfs:range  score:Asset .

sdom:has_pattern_role a rdf:Property ;
    rdfs:label "asset takes role in pattern"@en ;
    rdfs:domain score:Asset ;
    rdfs:range  sdom:PatternRole .

sdom:Pattern a rdfg:Graph ;
    rdfs:label "pattern"@en ;
    rdfs:description "Patterns are collections of assets (nodes) and relations between them (edges). In other words, they can be represented as RDF triples, as named graphs with identifiers."@en .

sdom:introduces a rdf:Property ;
    rdfs:label "introduces threat pattern"@en ;
    rdfs:description "Threat or control strategy introduces threat pattern"@en ;
    rdfs:domain score:Threat, score:ControlStrategy  ;
    rdfs:range  sdom:Pattern .
    
sdom:PatternRole a skos:ConceptScheme, rdfs:Class ;
    dct:title "Pattern role"@en .
    
sdom:pattern_role_unique a skos:Concept, sdom:PatternRole ;
    skos:inScheme   sdom:PatternRole ;
    skos:prefLabel  "unique"@en ;
    rdfs:label      "'unique' pattern role"@en .
sdom:pattern_role_necessary a skos:Concept, sdom:PatternRole ;
    skos:inScheme   sdom:PatternRole ;
    skos:prefLabel  "necessary"@en ;
    skos:altLabel   "redundant"@en ;
    rdfs:label      "'redundant' pattern role"@en .
sdom:pattern_role_sufficient a skos:Concept, sdom:PatternRole ;
    skos:inScheme   sdom:PatternRole ;
    skos:prefLabel  "sufficient"@en ;
    rdfs:label      "'sufficient' pattern role"@en .
sdom:pattern_role_prohibited a skos:Concept, sdom:PatternRole ;
    skos:inScheme   sdom:PatternRole ;
    skos:prefLabel  "prohibited"@en ;
    rdfs:label      "'prohibited' pattern role"@en .
